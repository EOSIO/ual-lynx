{
  "name": "ual-lynx",
  "version": "0.3.0",
  "main": "dist/index.js",
  "license": "MIT",
  "author": {
    "name": "Block.one",
    "url": "https://block.one"
  },
  "contributors": [
    "Eddie Sheffield",
    "Nasser Abouelazm",
    "Chris Allnutt",
    "Randy Torres",
    "Jeffrey Smith II",
    "Tara Tritt"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prepare": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "eosjs": "20.0.3",
    "universal-authenticator-library": "0.2.0"
  },
  "devDependencies": {
    "@blockone/tslint-config-blockone": "^4.0.1",
    "@types/jest": "^25.1.4",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/dist"
    ],
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "jsdom"
  }
}
